<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.qf.bestwedding">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/NoActionBar">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.qf.flag.toolsfragment.HeadWedding"/>
        <activity android:name="com.qf.flag.toolsfragment.InvitationActivity"/>

        <!--城市选择页面-->
        <activity android:name=".CitySelectActivity"/>

        <!--主页面MainActivity-->
        <activity android:name=".MainActivity"/>

        <!--引导页面GuideActivity-->
        <activity android:name=".GuideActivity"/>

        <!--金融超市-->
        <activity android:name=".FinancialActivity"/>
        <!--发现页图片跳转页面-->
        <activity android:name="com.qf.flag.discoverfragment.DiscoverJumpActivity"/>
    </application>

</manifest>